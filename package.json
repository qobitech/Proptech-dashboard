{
	"name": "proptech",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite --open",
		"build": "tsc && vite build",
		"preview": "vite preview",
		"check-types": "tsc --noEmit --pretty",
		"lint:eslint-check": "eslint . --report-unused-disable-directives --max-warnings 0",
		"lint:eslint-fix": "eslint . --fix --report-unused-disable-directives --max-warnings 0",
		"lint:prettier-check": "prettier '**/*' --check --ignore-unknown",
		"lint:prettier-fix": "prettier '**/*' --write --ignore-unknown",
		"lint:check": "tsc && yarn lint:eslint-check && yarn lint:prettier-check",
		"lint:fix": "yarn lint:eslint-fix && yarn lint:prettier-fix",
		"test": "vitest --run --typecheck",
		"test:watch": "vitest --watch --typecheck --coverage",
		"test:coverage": "yarn test --coverage",
		"test:coverage-ui": "yarn test --coverage",
		"test:update": "yarn test -u"
	},
	"dependencies": {
		"react": "^19.2.0",
		"react-dom": "^19.2.0",
		"react-helmet-async": "^2.0.5",
		"react-router": "^7.6.3"
	},
	"devDependencies": {
		"@eslint/js": "^9.29.0",
		"@stylistic/eslint-plugin": "^5.0.0",
		"@testing-library/dom": "^10.4.0",
		"@testing-library/jest-dom": "^6.6.4",
		"@testing-library/react": "^16.3.0",
		"@testing-library/user-event": "^14.6.1",
		"@types/eslint-plugin-jsx-a11y": "^6",
		"@types/lodash": "^4",
		"@types/node": "^24.0.6",
		"@types/react": "^19.1.8",
		"@types/react-dom": "^19.1.6",
		"@vitejs/plugin-react": "^4.5.2",
		"@vitest/coverage-v8": "^3.2.4",
		"@vitest/eslint-plugin": "^1.3.3",
		"@vitest/ui": "^3.2.4",
		"eslint": "^9.29.0",
		"eslint-config-prettier": "^10.1.5",
		"eslint-plugin-jsx-a11y": "^6.10.2",
		"eslint-plugin-prettier": "^5.5.1",
		"eslint-plugin-react": "^7.37.5",
		"eslint-plugin-react-hooks": "^5.2.0",
		"eslint-plugin-react-refresh": "^0.4.20",
		"eslint-plugin-simple-import-sort": "^12.1.1",
		"eslint-plugin-testing-library": "^7.5.3",
		"globals": "^16.5.0",
		"jsdom": "^26.1.0",
		"msw": "^2.12.2",
		"prettier": "^3.6.2",
		"sass": "1.77.6",
		"typescript": "~5.9.3",
		"typescript-eslint": "^8.46.4",
		"vite": "^7.2.4",
		"vitest": "^3.2.4"
	}
}
